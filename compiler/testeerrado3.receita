receita {
    var1 : 50 ml;
    var2 : [var1 / 5] g;
    var3 : [var1 - var2] ml;

    servir var1;
    servir var2;
    servir var3;

    anotar "Digite outro número:" var4;
    servir var4;

    picar se [var2 < var1] tambem [var3 > 10],
        servir "Condições "satisfeitas";
    senao,
        servir "Condições não satisfeitas";
    pare de picar;
}
